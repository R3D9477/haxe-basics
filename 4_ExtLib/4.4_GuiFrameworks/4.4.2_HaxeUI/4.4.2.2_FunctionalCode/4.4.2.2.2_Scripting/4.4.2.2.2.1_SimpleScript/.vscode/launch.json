{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug (Chromium)",
			"type": "chrome",
			"request": "launch",
			"preLaunchTask": "build html5 (debug)",
			"file": "${workspaceRoot}/Export/Html5/Debug/Main.html",
			"runtimeExecutable": "chromium-browser",
			"linux": {
				"runtimeExecutable": "/usr/bin/chromium-browser",
				"runtimeArgs": [ "--password-store=basic" ]
			},
			"timeout": 100000,
			"sourceMaps": true
		}
	]
}